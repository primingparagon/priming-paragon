PORT=4001

# Database (split privilege)
DATABASE_URL_READONLY=postgresql://postgres:<READ_ONLY_PASSWORD>@db.<PROJECT>.supabase.co:5432/postgres
DATABASE_URL_SERVICE=postgresql://service_role:<SERVICE_PASSWORD>@db.<PROJECT>.supabase.co:5432/postgres
SUPABASE_DB_ROLE=service_role

# Supabase
SUPABASE_URL=https://<PROJECT>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=<SUPABASE_SERVICE_ROLE_KEY>

# JWT (rotation-ready)
JWT_SECRET_V1=...
JWT_SECRET_V2=...
JWT_ACTIVE_SECRET=V2

JWT_REFRESH_SECRET_V1=...
JWT_REFRESH_SECRET_V2=...
JWT_REFRESH_ACTIVE_SECRET=V2

JWT_ISSUER=primingparagon.auth
JWT_AUDIENCE=primingparagon.api

# Anomaly Engine
ANOMALY_THRESHOLD_ADMIN=40
ANOMALY_BATCH_SIZE=50
ANOMALY_RETRY_COUNT=3
ANOMALY_RETRY_BASE_DELAY_MS=100
BLOCK_ANOMALOUS_ACTIONS=false

# Observability
OTEL_SERVICE_NAME=auth-service
OTEL_TRACES_EXPORTER=otlp

# Crypto hardening
NODE_OPTIONS=--enable-fips

# AI keys (already isolated)
OPENAI_API_KEY=<OPENAI_API_KEY>
GEMINI_API_KEY=<GEMINI_API_KEY>
PORT=4001
DATABASE_URL=postgresql://postgres:<PASSWORD>@db.<SUPABASE_PROJECT>.supabase.co:5432/postgres
SUPABASE_URL=https://<SUPABASE_PROJECT>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=<SUPABASE_SERVICE_ROLE_KEY>
JWT_SECRET=<JWT_SECRET>
JWT_REFRESH_SECRET=<JWT_REFRESH_SECRET>
OPENAI_API_KEY=<OPENAI_API_KEY>
GEMINI_API_KEY=<GEMINI_API_KEY>
